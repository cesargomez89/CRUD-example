<h1>Articles</h1>
<table>
  <thead>
    <th>Title</th>
    <th>Content</th>
  </thead>
  <tbody>
    <% @articles.each do |article| %>
      <tr>
        <td><%= article.title %></td>
        <td><%= article.content %></td>
        <td><%= link_to "Edit", edit_article_path(article.id)  %></td>
        <td><%= link_to "Destroy", article_path(article.id),  method: "delete"  %></td>
      </tr>
    <% end  %>
    </tbody>
  </table>
  <%= button_to "create new article", new_article_path, method: "get"  %>

